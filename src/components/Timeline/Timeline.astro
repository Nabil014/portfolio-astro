---
import data from '../../data/data.json'
import ItemTimeline from '../Timeline/ItemTimeline.astro'
---

<ol class='relative border-l border-gray-700'>
  {
    data.timeline.map((item) => (
      <ItemTimeline
        title={item.title}
        date={item.date}
        description={item.description}
        href={item.href}
      />
    ))
  }
</ol>
